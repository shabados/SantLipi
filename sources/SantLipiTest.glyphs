   sub @Medial' by @Isolated;
} ISOLATED;

lookup INITIAL {
	sub iMatra-gurmukhi @Medial' by @Initial;
	ignore sub @AllMarks @Medial';
	ignore sub @AllLetters @Medial';
	sub @Medial' iiMatra-gurmukhi by @Isolated;
	sub @Medial' by @Initial;
} INITIAL;

