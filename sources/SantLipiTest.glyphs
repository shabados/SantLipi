	sub @Medial' ya-gurmukhi.post by @MedialBeforeYa;
	sub @Initial' ya-gurmukhi.post by @InitialBeforeYa;
	sub iMatra-gurmukhi @Medial' by @Isolated;
	sub @Medial' iiMatra-gurmukhi by @Initial;
} FINAL2;

lookup FINAL {
   ignore sub @Medial' @AllLetters;
   sub @Medial' by @Final;
} FINAL;

";
